{
	"name": "icybee/module-thumbnailer",
	"type": "icanboogie-module",
	"description": "Creates thumbnails on demand.",
	"keywords": [ "thumbnail" ],
	"homepage": "http://icybee.org/",
	"license": "BSD-3-Clause",

	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],

	"support": {
		"issues": "https://github.com/Icybee/module-thumbnailer/issues",
		"source": "https://github.com/Icybee/module-thumbnailer"
	},

	"minimum-stability": "dev",

	"require": {
		"php": ">=5.4.0",
		"icanboogie/module-installer": "*",
		"icanboogie/event": "*",
		"icanboogie/image": "*",
		"brickrouge/brickrouge": "2.x",
		"icybee/module-registry": "*"
	},

	"require-dev": {
		"icanboogie/icanboogie": "2.x",
		"icybee/core": "2.x",
		"icybee/module-cache": "2.x"
	},

	"suggest": {
		"icybee/module-cache": "Allows thumbnails cache management from Icybee's admin."
	},

	"autoload": {
		"psr-4": { "ICanBoogie\\Modules\\Thumbnailer\\": "lib" },
		"classmap": [ "lib/PopThumbnailVersion.php" ]
	},

	"scripts": {
		"post-autoload-dump": "ICanBoogie\\AutoConfig\\Generator::on_autoload_dump"
	}
}